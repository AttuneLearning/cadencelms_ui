{
  "$schema": "Learned team configuration from development session",
  "sourceSession": "memory/sessions/{date}-{slug}.md",
  "sourceIssue": "{TEAM}-ISS-{NNN}",
  "created": "YYYY-MM-DD",

  "issueProfile": {
    "issueType": "new-feature | bug-fix | refactor | integration | documentation",
    "complexity": "simple | moderate | complex",
    "filesModified": 0,
    "fsdLayers": ["entity", "feature", "widget", "page"],
    "crossTeam": false,
    "tags": []
  },

  "teamConfig": {
    "presetUsed": "solo | paired | standard | research | parallel-impl",
    "presetIdeal": "solo | paired | standard | research | parallel-impl",
    "teammates": [
      {
        "role": "implementer | tester | researcher",
        "taskCount": 0,
        "effectiveness": "excellent | good | adequate | poor"
      }
    ],
    "taskPattern": "newFeature | bugFix | complexFeature",
    "midDevAdjustments": "none | description of changes"
  },

  "results": {
    "effectiveness": "excellent | good | adequate | poor",
    "bottlenecks": "none | description",
    "recommendation": "Reuse this config for similar issues | Try {alternative} instead",
    "notes": ""
  }
}
